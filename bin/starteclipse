#!/bin/sh
#
# Workaround script for GTK+ vs. Eclipse problems in Karmic

BASE=`dirname $0`
REQUIRED="$REQUIRED java"

. $BASE/common/java

ECLIPSE_CMD=$BASE/packages/eclipse/eclipse

if [ ! -f "$ECLIPSE_CMD" ]; then
  echo "Could not find Eclipse in $ECLIPSE_CMD"
  exit 1
fi

$ECLIPSE_CMD -vm $JAVA_CMD "$@"
